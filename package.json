{
  "name": "@akaoio/tui",
  "version": "1.0.6",
  "description": "Simple and practical Terminal UI framework for Node.js",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "example:simple": "tsx examples/simple.ts",
    "example:form": "tsx examples/form.ts",
    "example:components": "tsx examples/components.ts",
    "example:demo": "tsx examples/demo.ts",
    "example:todo": "tsx examples/todos/TodoApp.ts",
    "example:todo-simple": "tsx examples/todos/TodoAppSimple.ts",
    "example:todo-schema": "tsx examples/todos/TodoFromSchema.ts",
    "example:file-explorer": "tsx examples/file-explorer/FileExplorerFromSchema.ts",
    "example:dashboard": "tsx examples/dashboard/DashboardFromSchema.ts",
    "example:dashboard-simple": "tsx examples/dashboard/SimpleDashboard.ts",
    "example:dashboard-app": "tsx examples/dashboard-app/dashboard.ts",
    "example:json-editor": "tsx examples/json-editor/config-editor.ts",
    "example:schema-form": "tsx examples/schema-form/app.ts",
    "example:meta-app": "tsx examples/meta-app/runtime.ts"
  },
  "keywords": [
    "tui",
    "terminal",
    "ui",
    "cli",
    "console",
    "keyboard",
    "navigation",
    "forms",
    "checkbox",
    "radio",
    "select",
    "input",
    "spinner",
    "progress"
  ],
  "author": "akaoio",
  "license": "MIT",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "examples"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/akaoio/tui.git"
  },
  "bugs": {
    "url": "https://github.com/akaoio/tui/issues"
  },
  "homepage": "https://github.com/akaoio/tui#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "tsup": "^8.0.2",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3"
  },
  "dependencies": {}
}
