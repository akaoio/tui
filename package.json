{
  "name": "@akaoio/tui",
  "version": "1.0.7",
  "description": "Simple and practical Terminal UI framework for Node.js",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "akao-build",
    "dev": "akao-build --watch",
    "lint": "tsc --noEmit",
    "test": "npm run build && npx tsx tests/battle/index.test.ts",
    "test:comprehensive": "npm run build && npx tsx tests/battle/comprehensive.battle.test.ts",
    "test:visual": "npm run build && npx tsx tests/battle/visual-test.ts",
    "test:visual-replay": "npm run build && npx tsx tests/battle/visual-replay.battle.test.ts",
    "test:integration": "npm run build && npx tsx tests/battle/integration-flows.battle.test.ts",
    "test:master": "npm run build && npx tsx tests/battle/master-test-suite.ts",
    "test:all": "npm run test:master",
    "test:watch": "npm run build && npx tsx tests/battle/index.test.ts --watch",
    "test:coverage": "npm run build && npx tsx tests/battle/index.test.ts --coverage",
    "test:old": "npm run build && bun test",
    "test:legacy": "npm run build && bun test",
    "docs": "composer build",
    "docs:build": "composer build",
    "docs:watch": "composer watch",
    "example:todo": "tsx examples/todos/TodoFromSchema.ts",
    "example:dashboard": "tsx examples/dashboard/DashboardFromSchema.ts",
    "example:json-editor": "tsx examples/json-editor/config-editor.ts",
    "example:composer": "tsx examples/composer-ui/index.ts",
    "example:battle": "tsx examples/battle-dashboard/index.ts",
    "example:builder": "tsx examples/builder-monitor/index.ts",
    "example:air": "tsx examples/air-client/index.ts",
    "example:unified": "node examples/unified-dashboard-simple.js",
    "example:unified-ts": "tsx examples/unified-dashboard/index.ts",
    "example:responsive": "node examples/responsive-showcase.js",
    "example:mobile": "node examples/mobile-optimized.js",
    "example:mobile-responsive": "node examples/mobile-responsive-simple.js",
    "example:mobile-responsive-ts": "tsx examples/mobile-responsive/index.ts",
    "example:core-integration": "node examples/core-tech-integration.js",
    "example:battle-tested": "node examples/battle-tested-dashboard.js",
    "example:unified-fixed": "node examples/unified-dashboard-fixed.js",
    "example:mobile-fixed": "node examples/mobile-optimized-fixed.js",
    "test:examples": "node tests/battle/final-test.js",
    "test:battle-all": "node tests/battle/real-examples-test.js && node tests/battle/final-test.js"
  },
  "keywords": [
    "tui",
    "terminal",
    "ui",
    "cli",
    "console",
    "keyboard",
    "navigation",
    "forms",
    "checkbox",
    "radio",
    "select",
    "input",
    "spinner",
    "progress"
  ],
  "author": "akaoio",
  "license": "MIT",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "examples"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/akaoio/tui.git"
  },
  "bugs": {
    "url": "https://github.com/akaoio/tui/issues"
  },
  "homepage": "https://github.com/akaoio/tui#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@akaoio/battle": "*",
    "@akaoio/builder": "*",
    "@akaoio/composer": "*",
    "@types/node": "^20.11.30",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3"
  },
  "dependencies": {}
}
