{
  "name": "@akaoio/tui",
  "version": "1.0.1",
  "description": "Simple and practical Terminal UI framework for Node.js",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "example:simple": "tsx examples/simple.ts",
    "example:form": "tsx examples/form.ts",
    "example:components": "tsx examples/components.ts"
  },
  "keywords": [
    "tui",
    "terminal",
    "ui",
    "cli",
    "console",
    "keyboard",
    "navigation",
    "forms",
    "checkbox",
    "radio",
    "select",
    "input",
    "spinner",
    "progress"
  ],
  "author": "akaoio",
  "license": "MIT",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/akaoagent/tui.git"
  },
  "bugs": {
    "url": "https://github.com/akaoagent/tui/issues"
  },
  "homepage": "https://github.com/akaoagent/tui#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "tsup": "^8.0.2",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3"
  },
  "dependencies": {}
}