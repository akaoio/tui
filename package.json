{
  "name": "@akaoio/tui",
  "version": "1.0.7",
  "description": "Simple and practical Terminal UI framework for Node.js",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "akao-build",
    "dev": "akao-build --watch",
    "lint": "tsc --noEmit",
    "test": "npm run build && bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "example:todo": "tsx examples/todos/TodoFromSchema.ts",
    "example:dashboard": "tsx examples/dashboard/DashboardFromSchema.ts",
    "example:json-editor": "tsx examples/json-editor/config-editor.ts"
  },
  "keywords": [
    "tui",
    "terminal",
    "ui",
    "cli",
    "console",
    "keyboard",
    "navigation",
    "forms",
    "checkbox",
    "radio",
    "select",
    "input",
    "spinner",
    "progress"
  ],
  "author": "akaoio",
  "license": "MIT",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "examples"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/akaoio/tui.git"
  },
  "bugs": {
    "url": "https://github.com/akaoio/tui/issues"
  },
  "homepage": "https://github.com/akaoio/tui#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@akaoio/builder": "*",
    "@types/node": "^20.11.30",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3"
  },
  "dependencies": {}
}
